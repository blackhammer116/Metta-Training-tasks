(= (addTwo $x $y) (+ $x $y))

(: map-expr (-> (-> $t $t) Expression Expression))
(= (map-expr $f $expr)
    (if (== $expr ()) ()
        (let* ( ($head (car-atom $expr))
                ($tail (cdr-atom $expr))
                ($head-new ($f $head))
                ($tail-new (map-expr $f $tail))
            )
        (cons-atom $head-new $tail-new)
    )
)
)
! (map-expr 1 (1,2,3))